<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
		"-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<classPathEntry
		location="./src/main/resources/h2-1.4.195.jar" />

	<context id="context1">
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Criteria" />
		</plugin>
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 请修改此处地址 -->
		<jdbcConnection connectionURL="jdbc:h2:./target/temp_db"
			driverClass="org.h2.Driver"
			userId="sa" password="" />
		<javaModelGenerator targetPackage="me.laiyijie.server.data.domain"
			targetProject="src/main/java" />
		<sqlMapGenerator targetPackage="me.laiyijie.server.data.mapper"
			targetProject="src/main/java" />
		<javaClientGenerator targetPackage="me.laiyijie.server.data.dao"
			targetProject="src/main/java" type="XMLMAPPER" />

		<table  tableName="user" domainObjectName="TbUser">
			<property name="useActualColumnNames" value="true" />
		</table>

		<table  tableName="production" domainObjectName="TbProduction">
			<property name="useActualColumnNames" value="true" />
		</table>

	</context>
</generatorConfiguration>
