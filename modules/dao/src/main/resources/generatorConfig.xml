<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
		"-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<classPathEntry
		location="./src/main/resources/mysql-connector-java-5.1.42.jar" />

	<context id="context1">
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Criteria" />
		</plugin>
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 请修改此处地址 -->
		<jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/bojiu"
			driverClass="com.mysql.jdbc.Driver" password="laiyijie9"
			userId="root" />
		<javaModelGenerator targetPackage="net.bojiu.server.data.domain"
			targetProject="src/main/java" />
		<sqlMapGenerator targetPackage="net.bojiu.server.data.mapper"
			targetProject="src/main/java" />
		<javaClientGenerator targetPackage="net.bojiu.server.data.dao"
			targetProject="src/main/java" type="XMLMAPPER" />

		<table schema="bojiu" tableName="user" domainObjectName="tbUser">
			<property name="useActualColumnNames" value="true" />
		</table>

		<table schema="production" tableName="user" domainObjectName="tbProduction">
			<property name="useActualColumnNames" value="true" />
		</table>

	</context>
</generatorConfiguration>
